import Foundation
class DLDefaultTextField1: UITextField {
    required init?(coder aDecoder: NSCoder) {
        super.init(coder: aDecoder)
        self._setup()
    }
    func _setup() {
        self.setValue(TEXTHGARY_1, forKeyPath: "placeholderLabel.textColor")
        self.textColor = TEXTDARK_1
        self.font = UIFont.init(name: "PingFangSC-Regular", size: 15)
        self.borderStyle = .none
    }
}


